<?xml version='1.0' encoding='utf-8'?>
<Root>
    <TestCase name="agg_count_after_where">
        <Resource name="sqls">
            <Element>
<![CDATA[
SELECT brand, COUNT(*) FROM phone WHERE nid < 8 GROUP BY (brand)
]]>
</Element>
        </Resource>
        <Resource name="column_names">"brand", "COUNT(*)"</Resource>
        <Resource name="column_types">multi_char, uint64</Resource>
        <Resource name="seq_required">false</Resource>
        <Resource name="kvpair">databaseName:phone;</Resource>
        <Resource name="expect_data">
<![CDATA[
"Huawei", 2
"Meizu", 1
"Nokia", 1
"Apple", 1
"Xiaomi", 1
"OPPO", 1
]]>
</Resource>
<Resource name="plan">
<![CDATA[
SinkOp#4(type=api)
    ExchangeOp#3(distribution={type:SINGLETON})
        LimitOp#2(limit=100)
            AggregateOp#1(agg_funcs=[{approximate:false,distinct:false,filter_arg:-1,name:COUNT,output:[$COUNT(*)],type:NORMAL}],group_fields=[$brand],scope=NORMAL)
                TableScanOp#0(catalog_name=default,db_name=phone,limit=2147483647,output_fields=[$brand],push_down_ops=[{attrs:{condition:{op:<,params:[$nid,8],type:OTHER},output_fields:[$brand]},op_name:CalcOp}],table_name=phone,table_type=normal,use_nest_table=false)
]]>
</Resource>
</TestCase>
</Root>