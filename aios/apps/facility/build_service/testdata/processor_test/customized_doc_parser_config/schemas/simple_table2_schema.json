            "position_payload_flag" : 1,
            "position_list_flag" : 1,
            "index_analyzer" : "multi_level_analyzer",
            "index_fields": "title"
        },
	    {
	        "index_name": "pk",
	        "index_type": "PRIMARYKEY64",
	        "index_fields": "id"
	    }
    ],

    "attributes": ["price2"],

    "summarys":{
        "summary_fields": ["title",  "price2"],
        "compress_block_size": 5
    }
}
