{
    "build_option_config":
    {
        "async_build":
        true,
        "max_recover_time" : 120,
        "async_queue_mem":
            -1,
        "async_queue_size":
        10000,
        "batch_build_size":
        1,
        "build_qps_limit":
        0,
        "consistent_mode_build_thread_count":
            -1,
        "document_filter":
        true,
        "inconsistent_mode_build_thread_count":
            -1,
        "sleep_per_doc":
        0,
        "sort_build":
        ${sort_build},
        "sort_descriptions":
        ${sort_descriptions},
        "sort_queue_mem":
            2048,
        "sort_queue_size":
        10000000
    },
    "cluster_config":
    {
        "builder_rule_config":
        {
            "align_version":
            true,
            "build_parallel_num":
            1,
            "disable_inc_parallel_instance_dir":
            false,
            "inc_build_parallel_num":
            1,
            "map_reduce_ratio":
            1,
            "merge_parallel_num":
            1,
            "need_partition":
            true,
            "partition_count":
            ${partition_count},
            "partition_split_num":
            1
        },
        "cluster_name": "${table_name}",
        "hash_mode": ${hash_mode},
        "table_name": "${table_name}"
    },
    "disable_follower_build":
    false,
    "online_index_config":
    {
        "build_config": {
            "keep_version_count": 2,
            "max_doc_count": 1000000,
            "build_total_memory": 4096
        },
        "max_realtime_memory_use": 10240,
        "load_config": [
            {
                "warmup_strategy": "sequential",
                "file_patterns": [
                    "_INDEX_"
                ],
                "load_strategy": "mmap",
                "name": "_INDEX_",
                "load_strategy_param": {
                    "lock": false
                }
            },
            {
                "warmup_strategy": "none",
                "file_patterns": [
                    "_SUMMARY_"
                ],
                "load_strategy": "mmap",
                "name": "_SUMMARY_",
                "load_strategy_param": {
                    "lock": false
                }
            },
            {
                "warmup_strategy": "sequential",
                "file_patterns": [
                    "_ATTRIBUTE_"
                ],
                "load_strategy": "cache",
                "name": "_ATTRIBUTE_",
                "load_strategy_param": {
                    "global_cache": false,
                    "direct_io": false
                }
            }
        ]
    },
    "offline_index_config" : {
        "build_config" : {
            "build_total_memory" : 9216,
            "keep_version_count" : 10
        },
        "customized_merge_config": {
            "full": {
                "merge_config": {
                    "keep_version_count": 10,
                    "merge_strategy": "optimize",
                    "merge_strategy_param": "after-merge-max-segment-count=10"
                },
                "merge_parallel_num": 6
            },
            "inc": {
                "merge_config": {
                    "keep_version_count": 10,
                    "merge_strategy": "priority_queue",
                    "merge_strategy_params": {
                        "input_limits": "max-segment-size=60720",
                        "output_limits": "max-merged-segment-size=10240;max-total-merged-size=20480",
                        "strategy_conditions": "priority-feature=delete-doc-count#desc;conflict-segment-count=10000;conflict-delete-percent=10"
                    },
                    "merge_thread_count": 4
                },
                "period": "period=1800"
            }
        }
    },
    "realtime":
    true,
    "swiftConfig":
    {
        "reader_config":
        "oneRequestReadCount=50000;readBufferSize=50000;decompress=true;retryGetMsgInterval=1000000",
        "swift_client_config":
        "",
        "topic_full":
        {
            "compressMsg":
            false,
            "deleteTopicData":
            false,
            "needFieldFilter":
            false,
            "obsoleteFileTimeInterval":
                -1,
            "partitionCount":
            8,
            "partitionFileBufferSize":
            64,
            "partitionLimit":
            100000,
            "partitionMaxBufferSize":
            12288,
            "partitionMinBufferSize":
            8,
            "reservedFileCount":
                -1,
            "resource":
            10
        },
        "topic_inc":
        {
            "compressMsg":
            false,
            "deleteTopicData":
            false,
            "needFieldFilter":
            false,
            "obsoleteFileTimeInterval":
                -1,
            "partitionCount":
            8,
            "partitionFileBufferSize":
            64,
            "partitionLimit":
            100000,
            "partitionMaxBufferSize":
            12288,
            "partitionMinBufferSize":
            8,
            "reservedFileCount":
                -1,
            "resource":
            10
        },
        "writer_config":
        "functionChain=hashId2partId;compressMsg=true;compressThresholdInBytes=50;mode=async|safe;maxBufferSize=4294967296"
    }
}
