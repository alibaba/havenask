{
    "build_option_config":
    {
        "async_build":
        true,
        "async_queue_mem":
            -1,
        "async_queue_size":
        10000,
        "batch_build_size":
        1,
        "build_qps_limit":
        0,
        "consistent_mode_build_thread_count":
            -1,
        "document_filter":
        true,
        "inconsistent_mode_build_thread_count":
            -1,
        "sleep_per_doc":
        0,
        "sort_build":
        ${sort_build},
        "sort_descriptions":
        ${sort_descriptions},
        "sort_queue_mem":
            2048,
        "sort_queue_size":
        10000000
    },
    "cluster_config":
    {
        "builder_rule_config":
        {
            "align_version":
            true,
            "build_parallel_num":
            1,
            "disable_inc_parallel_instance_dir":
            false,
            "inc_build_parallel_num":
            1,
            "map_reduce_ratio":
            1,
            "merge_parallel_num":
            1,
            "need_partition":
            true,
            "partition_count":
            ${partition_count},
            "partition_split_num":
            1
        },
        "cluster_name": "${table_name}",
        "hash_mode": ${hash_mode},
        "table_name": "${table_name}"
    },
    "disable_follower_build":
    false,
    "online_index_config":
    {
        "build_config":
        {
            "keep_version_count":
            2,
            "max_doc_count":
            1000000
        }
    },
    "offline_index_config" : {
        "build_config" : {
            "build_total_memory" : 9216,
            "keep_version_count" : 10
        }
    },
    "realtime":
    true,
    "direct_write": true,
    "wal_config": {
        "strategy": "realtime_swift",
        "sink": {
            "src": "swift",
            "type": "swift",
            "swift_root": "${swift_root}",
            "topic_name": "${topic_name}",
            "writer_config": "needTimestamp=true;functionChain=hashId2partId;compressThresholdInBytes=1024;mode=async|safe;maxBufferSize=2147483648;maxKeepInBufferTime=10000"
        },
        "timeout_ms": 10000
    },
    "realtime_info": {
        "need_field_filter": "true",
        "topic_name": "${topic_name}",
        "writer_config": "functionChain=hashId2partId;maxKeepInBufferTime=10000",
        "realtime_mode": "realtime_service_rawdoc_rt_build_mode",
        "src_signature": "1",
        "reader_config": "retryGetMsgInterval=50000;partitionStatusRefreshInterval=10000",
        "type": "swift",
        "swift_root": "${swift_root}"
    }
}
